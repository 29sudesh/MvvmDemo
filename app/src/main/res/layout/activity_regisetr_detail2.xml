<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
        android:layout_height="match_parent" xmlns:app="http://schemas.android.com/apk/res-auto"
        android:orientation="vertical"
        tools:context=".view.activity.RegisterDetailActivity">

    <include layout="@layout/layout_common_toolbar"/>
    <android.support.v4.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
        <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginHorizontal="@dimen/_20sdp"
                android:padding="@dimen/_5sdp"
                android:orientation="vertical">
            <com.sb.ubergodriver.CommonUtils.CustomTextUtils.CustomTextView
                    android:fontFamily="helvetica.ttf"
                    android:text="@string/vehicle_name"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:singleLine="true"
                    android:textSize="@dimen/_15sdp"
                    android:gravity="start"
                    android:layout_marginTop="@dimen/_30sdp"
                    android:paddingStart="@dimen/_3sdp"
                    android:textColor="@color/toptextcolor"/>


                <com.sb.ubergodriver.CommonUtils.CustomTextUtils.CustomEditText
                        android:fontFamily="helvetica.ttf"
                        android:id="@+id/et_vehicle_name"
                        android:layout_height="@dimen/_40sdp"
                        android:layout_width="match_parent"
                        android:inputType="textPersonName|textNoSuggestions"
                        android:ems="10"
                        android:textSize="@dimen/_15sdp"
                        android:singleLine="true"
                        android:digits="ABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789abcdefghijklmnopqrstuvwxyz"
                        android:focusable="true"
                        android:onClick="onClick"
                        android:maxLength="64"
                        android:focusableInTouchMode="true"
                        android:textColorHint="@color/edittextline"
                        android:padding="@dimen/_10sdp"
                        android:imeOptions="actionNext"

                />


            <com.sb.ubergodriver.CommonUtils.CustomTextUtils.CustomTextView
                    android:fontFamily="helvetica.ttf"
                    android:text="@string/vehicle_number"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:singleLine="true"
                    android:textSize="@dimen/_15sdp"
                    android:gravity="start"
                    android:layout_marginTop="@dimen/_30sdp"
                    android:paddingStart="@dimen/_3sdp"
                    android:textColor="@color/toptextcolor"/>


            <com.sb.ubergodriver.CommonUtils.CustomTextUtils.CustomEditText
                    android:fontFamily="helvetica.ttf"
                    android:id="@+id/et_vehicle_number"
                    android:layout_height="@dimen/_40sdp"
                    android:layout_width="match_parent"
                    android:inputType="textPersonName|textNoSuggestions"
                    android:ems="10"
                    android:textSize="@dimen/_15sdp"
                    android:singleLine="true"
                    android:focusable="true"
                    android:maxLength="64"
                    android:digits="ABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789abcdefghijklmnopqrstuvwxyz"
                    android:onClick="onClick"
                    android:focusableInTouchMode="true"
                    android:textColorHint="@color/edittextline"
                    android:padding="@dimen/_10sdp"
                    android:imeOptions="actionNext"

            />

            <com.sb.ubergodriver.CommonUtils.CustomTextUtils.CustomTextView
                    android:fontFamily="helvetica.ttf"
                    android:text="@string/vehicle_type"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:singleLine="true"
                    android:textSize="@dimen/_15sdp"
                    android:gravity="start"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:paddingStart="@dimen/_3sdp"
                    android:textColor="@color/toptextcolor"/>


            <com.sb.ubergodriver.CommonUtils.CustomTextUtils.CustomEditText
                    android:fontFamily="helvetica.ttf"
                    android:id="@+id/et_vehicle_type"
                    android:layout_height="@dimen/_40sdp"
                    android:layout_width="match_parent"
                    android:inputType="textPersonName|textNoSuggestions"
                    android:ems="10"
                    android:textSize="@dimen/_15sdp"
                    android:singleLine="true"
                    android:focusable="false"
                    android:maxLength="64"
                    android:onClick="onClick"
                    android:digits="ABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789abcdefghijklmnopqrstuvwxyz"
                    android:focusableInTouchMode="false"
                    android:textColorHint="@color/edittextline"
                    android:padding="@dimen/_10sdp"
                    android:imeOptions="actionNext"

            />

            <com.sb.ubergodriver.CommonUtils.CustomTextUtils.CustomTextView
                    android:fontFamily="helvetica.ttf"
                    android:text="@string/vehicle_model"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:singleLine="true"
                    android:textSize="@dimen/_15sdp"
                    android:gravity="start"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:paddingStart="@dimen/_3sdp"
                    android:textColor="@color/toptextcolor"/>


            <com.sb.ubergodriver.CommonUtils.CustomTextUtils.CustomEditText
                    android:fontFamily="helvetica.ttf"
                    android:id="@+id/et_vehicle_model"
                    android:layout_height="@dimen/_40sdp"
                    android:layout_width="match_parent"
                    android:inputType="textPersonName|textNoSuggestions"
                    android:ems="10"
                    android:textSize="@dimen/_15sdp"
                    android:singleLine="true"
                    android:focusable="true"
                    android:maxLength="64"
                    android:digits="ABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789abcdefghijklmnopqrstuvwxyz"
                    android:onClick="onClick"
                    android:focusableInTouchMode="true"
                    android:textColorHint="@color/edittextline"
                    android:padding="@dimen/_10sdp"
                    android:imeOptions="actionNext"

            />

            <com.sb.ubergodriver.CommonUtils.CustomTextUtils.CustomTextView
                    android:fontFamily="helvetica.ttf"
                    android:text="@string/vehicle_manufacturing_name"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:singleLine="true"
                    android:textSize="@dimen/_15sdp"
                    android:gravity="start"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:paddingStart="@dimen/_3sdp"
                    android:textColor="@color/toptextcolor"/>


            <com.sb.ubergodriver.CommonUtils.CustomTextUtils.CustomEditText
                    android:fontFamily="helvetica.ttf"
                    android:id="@+id/et_vehicle_manufacturing_year"
                    android:layout_height="@dimen/_40sdp"
                    android:layout_width="match_parent"
                    android:inputType="textPersonName|textNoSuggestions"
                    android:ems="10"
                    android:textSize="@dimen/_15sdp"
                    android:singleLine="true"
                    android:focusable="true"
                    android:maxLength="64"
                    android:onClick="onClick"
                    android:digits="ABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789abcdefghijklmnopqrstuvwxyz"
                    android:focusableInTouchMode="true"
                    android:textColorHint="@color/edittextline"
                    android:padding="@dimen/_10sdp"
                    android:imeOptions="actionNext"

            />



            <com.sb.ubergodriver.CommonUtils.CustomTextUtils.CustomTextView
                    android:fontFamily="helvetica.ttf"
                    android:text="@string/vehicle_color"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:singleLine="true"
                    android:textSize="@dimen/_15sdp"
                    android:gravity="start"

                    android:layout_marginTop="@dimen/_10sdp"
                    android:paddingStart="@dimen/_3sdp"
                    android:textColor="@color/toptextcolor"/>


            <com.sb.ubergodriver.CommonUtils.CustomTextUtils.CustomEditText
                    android:fontFamily="helvetica.ttf"
                    android:id="@+id/et_vehicle_color"
                    android:layout_height="@dimen/_40sdp"
                    android:layout_width="match_parent"
                    android:inputType="textPersonName|textNoSuggestions"
                    android:ems="10"
                    android:textSize="@dimen/_15sdp"
                    android:singleLine="true"
                    android:focusable="true"
                    android:maxLength="64"
                    android:onClick="onClick"
                    android:digits="ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz"
                    android:focusableInTouchMode="true"
                    android:textColorHint="@color/edittextline"
                    android:padding="@dimen/_10sdp"
                    android:imeOptions="actionNext"

            />


            <com.sb.ubergodriver.CommonUtils.CustomTextUtils.CustomButton
                    android:fontFamily="helvetica.ttf"
                    android:id="@+id/btn_reset"
                    android:layout_width="@dimen/_40sdp"
                    android:layout_height="@dimen/_40sdp"
                    android:ems="10"
                    android:textSize="@dimen/_18sdp"
                    android:layout_marginTop="@dimen/_25sdp"
                    android:background="@drawable/ic_next_arrow_fab"
                    android:padding="@dimen/_8sdp"
                    android:textColor="@color/white"
                    android:layout_gravity="end"
                    android:textAllCaps="false"
                    android:singleLine="true"/>

            <!--bind:toastMessage="@{viewModel.toastMessage}"-->

        </LinearLayout>
    </android.support.v4.widget.NestedScrollView>
</LinearLayout>
